macro=doskey /MACROS
alias=type %USERPROFILE%\.macro
valias=nvim %USERPROFILE%\.macro
salias=doskey /macrofile=%USERPROFILE%\.macro
unalias=doskey $*=

u=u_gb.bat $*
uu=uu_gb.bat $*
uuu=uuu_gb.bat $*
uuuu=uuuu_gb.bat $*

cd=cd_gb.bat $*
git=git_gb.bat $*

pwd=cd
c=cls
env=set
vi=nvim $*
gccgo=gcc $*.c -o $* $t $* $t del $*.exe
gppgo=gcc -mwindows $*.cpp -o $* $t $* $t del $*.exe
d=dir /w /o:g $*
da=dir /w /o:g /a $*
ip=ipconfig $*
jis=chcp 932
utf-8=chcp 65001
history=doskey /history
wsl=wsl.exe $*
psx=powershell.exe $*
rmdir=rmdir /s $*
cat=type $*
cp=copy $*
